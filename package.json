{
  "name": "react-hookable",
  "description": "Collection of essential React Composition Utilities",
  "version": "1.0.0-beta.58",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs.js",
      "import": "./dist/index.es.js"
    }
  },
  "sideEffects": false,
  "license": "MIT",
  "scripts": {
    "commit": "cz",
    "prepare": "husky install",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint --ignore-path .gitignore .",
    "lint:tsc": "tsc --noEmit",
    "test": "run-p test:*",
    "test:unit": "jest",
    "build": "run-p build:*",
    "build:scripts": "node -r esbuild-register scripts/build.ts",
    "build:types": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "storybook": "start-storybook -p 6006",
    "docs": "build-storybook",
    "release": "semantic-release"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@chakra-ui/icons": "^1.0.15",
    "@chakra-ui/react": "^1.6.8",
    "@commitlint/cli": "^13.2.0",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/git": "^10.0.0",
    "@storybook/addon-actions": "^6.3.9",
    "@storybook/addon-essentials": "^6.3.9",
    "@storybook/addon-links": "^6.3.9",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/preact": "^6.3.9",
    "@swc/core": "^1.2.92",
    "@swc/jest": "^0.2.4",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.1",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.2",
    "@types/react": "^17.0.26",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "autoprefixer": "^10.3.7",
    "babel-loader": "^8.2.2",
    "clsx": "^1.1.1",
    "commitizen": "^4.2.4",
    "commitlint-config-gitmoji": "^2.2.5",
    "cz-emoji": "^1.3.1",
    "esbuild": "^0.13.3",
    "esbuild-register": "^3.0.0",
    "eslint": "^7.32.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "framer-motion": "^4",
    "husky": "^7.0.2",
    "hygen": "^6.1.0",
    "is-email": "^1.0.1",
    "jest": "^27.2.4",
    "lint-staged": "^11.2.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.9",
    "preact": "^10.5.14",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "semantic-release": "^18.0.0",
    "semantic-release-gitmoji": "^1.4.2",
    "tailwindcss": "^3.0.0-alpha.1",
    "tsc-alias": "1.6.3",
    "tsc-files": "^1.1.2",
    "typescript": "^4.4.3",
    "utilitypes": "^1.0.0-beta.4"
  },
  "author": {
    "name": "TomokiMiyauci",
    "email": "contact@miyauchi.dev",
    "url": "https://miyauchi.dev/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TomokiMiyauci/react-hookable.git"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/TomokiMiyauci/react-hookable#readme",
  "bugs": "https://github.com/TomokiMiyauci/react-hookable/issues",
  "funding": {
    "type": "patreon",
    "url": "https://www.patreon.com/tomoki_miyauci"
  },
  "keywords": [
    "react",
    "hook",
    "hooks",
    "use",
    "collection",
    "utility"
  ],
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "tsc-files --noEmit",
      "eslint --cache --fix"
    ],
    "*.{json,md,mdx}": "prettier -w"
  }
}
